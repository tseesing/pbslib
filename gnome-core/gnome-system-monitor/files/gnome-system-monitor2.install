. /usr/lib/ybs/funcs

files="/etc/gconf/schemas/gnome-system-monitor.schemas"

post_install() {
    gnome2_install_schema $files
        gtk-update-icon-cache -f -q -t
} 

pre_remove() {
    gnome2_uninstall_schema $files
}

post_upgrade() {
    gtk-update-icon-cache -f -q -t
}
    
post_downgrade() {
    post_upgrade
}

post_remove() {
    post_upgrade
}

