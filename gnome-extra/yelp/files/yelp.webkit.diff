+static void
+html_set_a11y (YelpHtml *html)
+{
+    gboolean caret;
+
+    caret = yelp_settings_get_caret ();
+    /* TODO Webkit version */
+}
+
+gboolean
+yelp_html_initialize (void)
+{
+    return TRUE;
+}
+
+void
+yelp_html_shutdown (void)
+{
+    /* Empty */
+}
