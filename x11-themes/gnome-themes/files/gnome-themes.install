. /usr/lib/ybs/funcs

post_install() {
    gtk-update-icon-cache -f -q -t Crux HighContrastInverse HighContrastLargePrintInverse \
                             LargePrint HighContrast HighContrastLargePrint HighContrast-SVG Mist
}

post_upgrade() {
    post_install
}

post_downgrade() {
    post_install
}

