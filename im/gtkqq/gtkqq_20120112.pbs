DESCRIPTION="A QQ client based on gtk2 uses WebQQ protocol."
HOMEPAGE="http://code.google.com/p/gtk2-qq/"
LICENSE="GPL"
PACKAGER="Zhongxin Huang <huangzhongxin@ivali.com>"

SRC_URI="git://github.com/kernelhcy/gtk2qq.git"

RDEPEND="gst-plugins-base gtk2 sqlite"
