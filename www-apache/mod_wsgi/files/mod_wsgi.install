. /usr/lib/ybs/funcs

post_install() {
	cat << ENDOFMESSAGE
=>
=>
=>  To install mod_wsgi, add the following line in 
=>  /etc/apache/httpd.conf file :
=>
=>  LoadModule wsgi_module modules/mod_wsgi.so
=>
=>  and restart/reload Apache.
=>
=>
ENDOFMESSAGE
}

post_upgrade() {
	post_install
}

post_downgrade() {
	post_install
}

