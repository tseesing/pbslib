DESCRIPTION="X11 core wire protocol and auxiliary headers"
HOMEPAGE="http://xorg.freedesktop.org/"
LICENSE="custom"
PACKAGER="Zhongxin Huang <huangzhongxin@ivali.com>"
GROUP="x11-proto"
YARCH="any"

SRC_URI="http://xorg.freedesktop.org/releases/individual/proto/"$N-$V".tar.bz2"

RDEPEND=""
BDEPEND=""

pbs_unpack() {
    dounpack
}

pbs_config() {    
    config+=""    
    doconfig
}

pbs_build() {
    domake
}

pbs_install() {
    domkinstall
    dodoc COPYING
}
